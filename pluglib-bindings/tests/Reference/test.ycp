// usage:
//   Y2DIR=`pwd` yast test.ycp

{

import "Ref";

list<integer> l = [0,8,333];
y2warning("l=%1", l);
Ref::RListInt(l);
y2warning("l=%1", l);

integer i = 8;
y2warning("i=%1", i);
Ref::RInt(i);
y2warning("i=%1", i);
Ref::PInt(i);
y2warning("i=%1", i);

// bnc#408829
integer l = 5000000000;
y2warning("l=%1", l);
Ref::RLLong(l);
y2warning("l=%1", l);
Ref::PLLong(l);
y2warning("l=%1", l);

boolean b = false;
y2warning("b=%1", b);
Ref::RBool(b);
y2warning("b=%1", b);
Ref::PBool(b);
y2warning("b=%1", b);

string s = "hu";
y2warning("s=%1", s);
Ref::RStr(s);
y2warning("s=%1", s);
Ref::PStr(s);
y2warning("s=%1", s);
Ref::CRStr(s);
y2warning("s=%1", s);
Ref::CPStr(s);
y2warning("s=%1", s);

integer e = 4;
y2warning("e=%1", e);
Ref::REnum(e);
y2warning("e=%1", e);
Ref::PEnum(e);
y2warning("e=%1", e);


}
